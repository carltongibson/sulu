<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for site in sitemap %}
{% if site.nodeType == 1 %}
{% set url = sitemap_url(site.url, site.locale) %}
{% if url != false %}
    <url>
        <loc>{{ url }}</loc>
        <lastmod>{{ site.changed|date("Y-m-d") }}</lastmod>
    </url>
{% endif %}
{% endif %}
{% endfor %}
</urlset>
